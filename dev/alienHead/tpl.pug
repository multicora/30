//- Mixins
mixin text
  .text Experiments with personal project logo. In result we got awesome combinations.
mixin image-1
  picture
    source(
      srcset="files/images/alien-head/1920/alien_head_01.jpg 1x, files/images/alien-head/1920/alien_head_01@2x.jpg 2x"
      media="(min-width: 1920px)")
    source(
      srcset="files/images/alien-head/1440/alien_head_01.jpg 1x, files/images/alien-head/1440/alien_head_01@2x.jpg 2x"
      media="(min-width: 1440px)")
    source(
      srcset="files/images/alien-head/1280/alien_head_01.jpg 1x, files/images/alien-head/1280/alien_head_01@2x.jpg 2x"
      media="(min-width: 1280px)")
    source(
      srcset="files/images/alien-head/1024/alien_head_01.jpg 1x, files/images/alien-head/1024/alien_head_01@2x.jpg 2x"
      media="(min-width: 1024px)")
    source(
      srcset="files/images/alien-head/Tablet/alien_head_01.jpg 1x, files/images/alien-head/Tablet/alien_head_01@2x.jpg 2x"
      media="(min-width: 768px)")
    img(
      src="files/images/alien-head/Mobile/alien_head_01.jpg"
      srcset="files/images/alien-head/Mobile/alien_head_01@2x.jpg 2x")

