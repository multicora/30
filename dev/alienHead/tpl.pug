//- Markup
.alienHead
  .clear-fix
    .title.size-2-col-6 Alien head
    .size-0-hide.size-2-col-6
      +text
  .heades.clear-fix.size-2-col-6
    .image.size-1-col-6.size-3-col-4
      +image-1
    .image.size-1-col-6.size-3-col-4
      +image-2
    .image.size-0-hide.size-1-col-6.size-2-offset-3.size-3-col-4.size-3-offset-8
      +image-4
  .size-min-1-hide
    +text

  .heades.clear-fix.size-2-col-6
    .image.size-1-col-6.size-2-col-6.size-2-offset-6.size-3-col-4.size-3-offset-0
      +image-3
    .image.size-1-col-6.size-min-1-hide
      +image-4
    .picture-5.image.size-1-col-6.size-1-offset-3.size-2-offset-0.size-3-col-4.size-3-offset-8
      +image-5